    "max_replica": 4,
    "cpu_request": "400m",
    "memory_request": "512Mi"
  },
  "autoscaling_policy": {
    "metric": "rps",
    "target": "100"
  },
  "timeout": "5s",
  "log_config": {
    "log_level": "INFO",
    "custom_metrics_enabled": false,
    "fiber_debug_log_enabled": false,
    "jaeger_enabled": false,
    "result_logger_type": "nop"
  }
